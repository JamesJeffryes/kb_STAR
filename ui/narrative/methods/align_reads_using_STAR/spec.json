{
    "ver": "0.0.1",
    "authors": [
        "qzhang"
    ],
    "contact": "help@kbase.us",
    "visible" : true,
    "categories": ["active","expression","sequence"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [{
        "id" : "sampleset_ref",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "text",
        "text_options" : {
            "valid_ws_types" :  [
		    "KBaseRNASeq.RNASeqSampleSet",
                    "KBaseSets.ReadsSet",
                    "KBaseFile.SingleEndLibrary",
                    "KBaseFile.PairedEndLibrary"
                ]
        }
    }, {
        "id" : "genome_ref",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "text",
        "text_options" : {
            "valid_ws_types" : ["KBaseGenomes.Genome"]
        }
     }, {
        "id": "output_obj_name",
        "optional": false,
        "advanced": false,
        "allow_multiple": false,
        "default_values": [""],
        "field_type": "text",
        "text_options": {
            "valid_ws_types" : ["KBaseRNASeq.RNASeqAlignment", "KBaseRNASeq.RNASeqAlignmentSet"],
            "is_output_name" : true
        }
    }, {
        "id": "reads_condition",
        "optional" : true,
        "advanced": false,
        "allow_multiple": false,
        "default_values": [ "unspecified" ],
        "field_type": "text"
    }, {
        "id": "quantMode",
        "optional": true,
        "advanced": true,
        "allow_multiple": false,
        "default_values": [ "TranscriptomeSAM" ],
        "field_type" : "dropdown",
        "dropdown_options":{
            "options": [{
                "value": "TranscriptomeSAM",
                "display": "SAM/BAM alignment to transcripts",
                "id": "TranscriptomeSAM",
                "ui_name": "TranscriptomeSAM"
            }, {
                "value": "GeneCounts",
                "display": "Gene Count",
                "id": "GeneCounts",
                "ui_name": "GeneCounts"
            },{
                "value": "Both",
                "display": "Both",
                "id": "Both",
                "ui_name": "Both"
            }]
        }
    }, {
        "id": "alignSJoverhangMin",
        "optional": true,
        "advanced": true,
        "allow_multiple": false,
        "default_values": [ "8" ],
        "field_type": "text",
        "text_options": {
            "validate_as": "int",
            "min_int" : 0
        }
   }, {
        "id": "alignSJDBoverhangMin",
        "optional": true,
        "advanced": true,
        "allow_multiple": false,
        "default_values": [ "1" ],
        "field_type": "text",
        "text_options": {
            "validate_as": "int",
            "min_int" : 0
        }
   }, {
        "id": "outFilterMismatchNmax",
        "optional": true,
        "advanced": true,
        "allow_multiple": false,
        "default_values": [ "999" ],
        "field_type": "text",
        "text_options": {
            "validate_as": "int",
            "min_int" : 0
        }
    }, {
        "id": "alignIntronMin",
        "optional": true,
        "advanced": true,
        "allow_multiple": false,
        "default_values": [ "20" ],
        "field_type": "text",
        "text_options": {
            "validate_as": "int",
            "min_int" : 0
        }
    }, {
        "id": "alignIntronMax",
        "optional": true,
        "advanced": true,
        "allow_multiple": false,
        "default_values": [ "1000000" ],
        "field_type": "text",
        "text_options": {
            "validate_as": "int",
            "min_int" : 0
        }
    }, {
        "id": "alignMatesGapMax",
        "optional": true,
        "advanced": true,
        "allow_multiple": false,
        "default_values": [ "1000000" ],
        "field_type": "text",
        "text_options": {
            "validate_as": "int",
            "min_int" : 0
        }
    }, {
        "id" : "output_alignment_filename_extension",
        "optional" : true,
        "advanced" : true,
        "allow_multiple" : false,
        "default_values" : [ "_starAlignment" ],
        "field_type" : "text",
        "text_options" : { 
            "is_output_name" : true
        }   
   }],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name":"STAR",
            "method": "run_star",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "output_workspace"
                }, {
                    "constant_value": 1,
                    "target_property": "create_report"
                }, {            
                    "constant_value"  : 2,
                    "target_property" : "runThreadN"
                }, {
                    "input_parameter" : "sampleset_ref",
                    "target_property" : "readsset_ref",
                    "target_type_transform": "resolved-ref"
                }, {
                    "input_parameter" : "genome_ref",
                    "target_property" : "genome_ref",
                    "target_type_transform": "resolved-ref"
                }, {
                    "input_parameter" : "output_obj_name",
                    "target_property" : "output_name"
                }, {
                    "input_parameter": "quantMode",
                    "target_property": "quantMode"
                }, {
                    "input_parameter": "alignSJoverhangMin",
                    "target_property": "alignSJoverhangMin"
                }, {
                    "input_parameter": "alignSJDBoverhangMin",
                    "target_property": "alignSJDBoverhangMin"
                }, {
                    "input_parameter": "outFilterMismatchNmax",
                    "target_property": "outFilterMismatchNmax"
                }, {
                    "input_parameter": "alignIntronMin",
                    "target_property": "alignIntronMin"
                }, {
                    "input_parameter": "alignIntronMax",
                    "target_property": "alignIntronMax"
                }, {
                    "input_parameter": "alignMatesGapMax",
                    "target_property": "alignMatesGapMax"
                }, {
                    "input_parameter" : "reads_condition",
                    "target_property" : "condition"
                }, {
                    "input_parameter" : "output_alignment_filename_extension",
                    "target_property" : "output_alignment_filename_extension"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"alignment_ref"],
                    "target_property": "output"
                },
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "output_workspace"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
