{
    "ver": "0.0.1",
    "authors": [
        "qzhang"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [{
        "id" : "sampleset_ref",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "text",
        "text_options" : {
            "valid_ws_types" : ["KBaseSets.ReadsSet", "KBaseRNASeq.RNASeqSampleSet","KBaseAssembly.SingleEndLibrary", "KBaseAssembly.PairedEndLibrary", "KBaseFile.SingleEndLibrary", "KBaseFile.PairedEndLibrary", "KBaseFile.SingleEndLibrary-2.0", "KBaseFile.PairedEndLibrary-2.0", "KBaseFile.SingleEndLibrary-2.1", "KBaseFile.PairedEndLibrary-2.1"]
        }
    }, {
        "id" : "genome_ref",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "text",
        "text_options" : {
            "valid_ws_types" : ["KBaseGenomes.Genome"]
        }
     }, {
        "id": "alignment_output",
        "optional": false,
        "advanced": false,
        "allow_multiple": false,
        "default_values": [""],
        "field_type": "text",
        "text_options": {
            "is_output_name": true
        }
    }, {
        "id": "quantMode",
        "optional": true,
        "advanced": true,
        "allow_multiple": false,
        "default_values": [ "TranscriptomeSAM" ],
        "field_type" : "dropdown",
        "dropdown_options":{
            "options": [{
                "value": "TranscriptomeSAM",
                "display": "SAM/BAM alignment to transcripts",
                "id": "TranscriptomeSAM",
                "ui_name": "TranscriptomeSAM"
            }, {
                "value": "GeneCounts",
                "display": "Gene Count",
                "id": "GeneCounts",
                "ui_name": "GeneCounts"
            },{
                "value": "Both",
                "display": "Both",
                "id": "Both",
                "ui_name": "Both"
            }]
        }
    }, {
            "id": "alignSJoverhangMin",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "8" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int" : 0
            }
   }, {
            "id": "alignSJDBoverhangMin",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int" : 0
            }
   }, {
            "id": "outFilterMismatchNmax",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "999" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int" : 0
            }
    }, {
            "id": "alignIntronMin",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "20" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int" : 0
            }
    }, {
            "id": "alignIntronMax",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1000000" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int" : 0
            }
    }, {
            "id": "alignMatesGapMax",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1000000" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int" : 0
            }
    }, {
            "id": "outFileNamePrefix",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "STARoutput_" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "text"
            }
    }],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name":"STAR",
            "method": "run_star",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }, {
                    "input_parameter" : "sampleset_ref",
                    "target_property" : "sampleset_ref",
                    "target_type_transform": "resolved-ref"
                }, {
                    "input_parameter" : "genome_ref",
                    "target_property" : "genome_ref",
                    "target_type_transform": "resolved-ref"
                }, {
                    "input_parameter" : "alignment_output",
                    "target_property" : "alignmentset_name"
                }, {
                    "input_parameter": "quantMode",
                    "target_property": "quantMode"
                }, {
                    "input_parameter": "alignSJoverhangMin",
                    "target_property": "alignSJoverhangMin"
                }, {
                    "input_parameter": "alignSJDBoverhangMin",
                    "target_property": "alignSJDBoverhangMin"
                }, {
                    "input_parameter": "outFilterMismatchNmax",
                    "target_property": "outFilterMismatchNmax"
                }, {
                    "input_parameter": "alignIntronMin",
                    "target_property": "alignIntronMin"
                }, {
                    "input_parameter": "alignIntronMax",
                    "target_property": "alignIntronMax"
                }, {
                    "input_parameter": "alignMatesGapMax",
                    "target_property": "alignMatesGapMax"
                }, {
                    "input_parameter": "outFileNamePrefix",
                    "target_property": "outFileNamePrefix"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"alignment_ref"],
                    "target_property": "output"
                },
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
